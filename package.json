{
  "name": "clang-time-tracer",
  "displayName": "Clang Time Tracer",
  "description": "",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Other"
  ],
  "publisher": "Glordim",
  "repository": {
    "type": "git",
    "url": "https://github.com/Glordim/vscode-clang-time-tracer.git"
  },
  "license": "MIT",
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "clang_time_tracer.analyze",
        "title": "Clang Time Tracer: Analyze"
      },
      {
        "command": "clang_time_tracer.build_and_analyze",
        "title": "Clang Time Tracer: Build and analyze"
      }
    ],
    "configuration": {
      "title": "Clang Time Tracer",
      "properties": {
        "clangTimeTracer.compileCommands.path": {
          "type": "string",
          "default": "build",
          "description": "Path to the compile_commands.json.",
          "format": "path"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.108.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.52.0",
    "eslint": "^9.39.2",
    "esbuild": "^0.27.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}